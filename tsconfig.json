{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "es2016",
      "dom"
    ],
    "jsx": "react-native",
    "module": "commonjs",
    "resolveJsonModule": true,
    "noEmit": true,
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "services/**/*"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
